{
  "name": "react-mock-api",
  "version": "1.0.0",
  "description": "React Mock API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaughanpo/react-mock-api.git"
  },
  "keywords": [],
  "author": "Vaughan Po",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vaughanpo/react-mock-api/issues"
  },
  "homepage": "https://github.com/vaughanpo/react-mock-api#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.5.2",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
